{
  "name": "ivanilsonsousa/agnostic-pdf",
  "description": "Manipulação de arquivos PDF agnóstica de drivers para aplicações Laravel",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "AgnosticPDF\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require": {
    "dompdf/dompdf": "^3.1",
    "mpdf/mpdf": "^8.2",
    "laravel/framework": "^12.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^12.3",
    "friendsofphp/php-cs-fixer": "^3.85",
    "mockery/mockery": "^1.6",
    "orchestra/testbench": "^10.5",
    "fakerphp/faker": "^1.23"
  },
  "extra": {
    "laravel": {
      "providers": [
        "AgnosticPDF\\AgnosticPDFServiceProvider"
      ]
    }
  }
}
